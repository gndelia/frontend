{
  "name": "utnianos-frontend",
  "version": "0.0.1",
  "description": "Plataforma de UTNianos",
  "main": "index.js",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "enzyme": "^2.9.1",
    "eslint": "3",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "react-addons-test-utils": "15.x.x",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "2.2.x",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "antd": "^2.11.2",
    "babel-core": "^6.25.0",
    "babel-plugin-import": "^1.2.1",
    "babel-polyfill": "^6.13.0",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.2",
    "cssnano": "^3.7.4",
    "file-loader": "^0.11.2",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react": "^15.6.1",
    "react-css-modules": "^4.3.0",
    "react-dom": "^15.6.1",
    "react-fa": "^4.2.0",
    "react-loader": "^2.4.2",
    "react-redux": "^5.0.5",
    "react-responsive": "^1.3.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-slick": "^0.14.11",
    "react-test-render": "^1.0.3",
    "react-transition-group": "1.x",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "redux-implicit-oauth2": "^1.0.2",
    "reselect": "^3.0.1",
    "sass-loader": "^6.0.6"
  },
  "scripts": {
    "start": "webpack-dev-server --history-api-fallback --progress --host 0.0.0.0 --port 3000 --inline --hot --content-base build/",
    "build": "webpack --progress --colors -d",
    "build-prod": "webpack --progress --display-modules -p --display-reasons",
    "test": "jest --env=jsdom",
    "test:coverage": "jest --env=jsdom --coverage && codecov",
    "lint": "eslint",
    "lint-all": "eslint src/ test/",
    "lint-all-ci": "eslint -f checkstyle src/ test/ > eslint.xml",
    "check-deps": "npm-check --skip-unused; exit 0;",
    "clean": "exit 0;"
  },
  "jest": {
    "setupFiles": [
      "./setupTests.js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": false,
    "collectCoverageFrom" : ["src/**/*.{js,jsx}", "!**/node_modules/**", "!**/vendor/**"],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UTNianos/frontend.git"
  },
  "author": "UTNianos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UTNianos/frontend/issues"
  },
  "homepage": "https://github.com/UTNianos/frontend#readme"
}
